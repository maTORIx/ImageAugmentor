{
    "name": "flip_vertical",
    "dataset_types": ["image_classification", "image_object_detection", "image_pix2pix"],
    "sample_images": {
        "before": "/augmentors/skelton/sample.png",
        "after": "/augmentors/skelton/sample.png"
    },
    "params": [
        {
            "name": "skelton_percentage",
            "type": "number",
            "unit": "%",
            "min": 0,
            "max": 100,
            "step": 1,
            "default": 100
        },
        {
            "name": "skelton_range",
            "type": "range",
            "unit": "%",
            "min": 0,
            "max": 100,
            "step": 1,
            "min_default": 100,
            "max_default": 100
        },
        {
            "name": "skelton_select",
            "type": "select",
            "choises": ["true", "false"],
            "default": "true"
        }
    ]
}